<script>
	$('#add_rss').hide();
</script>
<table class="table table-hover table-bordered" style='margin-top: 5%'>
  <thead>
    <tr>
      <th>Feed URL</th>
      <th>Posted On</th>
    </tr>
  </thead>
  <tbody>
		<% @feeds.each do |feed| %>
			<tr>
				<td><h4 class="itemtitle"><a href=<%= feed.href_url %> target='_blank'><%= feed.title %></a></h4></td>
				<td><h5 class="itemposttime"><span>Posted: </span><%= feed.published_at.strftime('%d/%m/%Y %H:%M')%></h5></td>
			</tr>
		<% end %>
  </tbody>
</table>







