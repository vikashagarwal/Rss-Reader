<script>
	$('#add_rss').hide();
</script>
<table class='table table-hover table-bordered' style='margin-top: 5%'>
  <thead>
    <tr>
      <th class='col-sm-8'>Feed URL</th>
      <th class='col-sm-4'>Posted On</th>
    </tr>
  </thead>
  <tbody>
		<% @feeds.each do |feed| %>
			<tr>
				<td><a href=<%= feed.href_url %> target='_blank'><%= feed.title %></a></td>
				<td><%= feed.published_at.strftime('%d/%m/%Y %H:%M')%></td>
			</tr>
		<% end %>
  </tbody>
</table>
